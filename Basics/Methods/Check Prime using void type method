import java.util.Scanner;
public class Main{
   static void Prime(int x){
     int flag=0;
    for(int i=2;i<=x/2;i++){
      if(x%i==0){
        flag=1;
      }
    }
     if(flag==1)
     System.out.println(x+"is not prime");
     else
     System.out.println(x+"is prime");
   }
      
  public static void main(String args[]){
    Scanner sc= new Scanner(System.in);
    int num=sc.nextInt();
    Prime(num);
  }
}
