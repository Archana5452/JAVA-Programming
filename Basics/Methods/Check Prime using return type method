import java.util.Scanner;
public class Main{
   static boolean Prime(int x){
    for(int i=2;i<=x/2;i++){
      if(x%i==0){
        return false;
      }
    }
     return true;
   }
      
  public static void main(String args[]){
    Scanner sc= new Scanner(System.in);
    int num=sc.nextInt();
     System.out.println(Prime(num));
  }
}
